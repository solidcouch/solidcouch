    "@typescript-eslint/parser": "^5.60.0",
    "chai-subset": "^1.6.0",
    "concurrently": "^8.0.1",
    "cypress": "^12.12.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "start-server-and-test": "^2.0.0",
    "utility-types": "^3.10.0"
  }
